<%= div_for article do %> 
  <h3>
    <%= link_to article.title, article %> 
    <span class="actions">
      <%= link_to "edit", edit_article_path(article) %> 
      <%= link_to "delete",article, method: :delete, data: {confirm: "seguro?"} %> 
    </span>
  </h3>
   <%= article.body %> 
<% end %> 
